{
    "name": "heydon/robo-twig",
    "description": "Allow the processing of templates via robo",
    "type": "robo-tasks",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Gordon Heydon",
            "email": "gordon@heydon.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "consolidation/robo": "^2.2",
        "twig/twig": "^2.12.0"
    },
    "autoload": {
        "psr-4": {
            "Heydon\\Robo\\Task\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 3600,
        "discard-changes": true
    }
}
